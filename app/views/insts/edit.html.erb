<div class="container">
  <div class="col-md-2 col-md-offset-3 col-sm-11 ">
  <br>
  <%= link_to "アウスタの一覧へ", insts_path, class: 'btn btn-default btn-sm btn-info' %>
  <br>
<%= form_for(@inst) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>
  <%= f.label :content %>
  <%= f.text_field :content %>
  <br>
  <%= f.label :画像 %>
  <%= f.file_field :image %>
  <br>
  <%= f.submit %>
<% end %>


  </div>
</div<